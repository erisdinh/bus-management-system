        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <PasswordField fx:id="textfieldNewPass" GridPane.rowIndex="1" />
            <PasswordField fx:id="textfieldReNewPass" GridPane.rowIndex="3" />
            <Label text="Enter new password:" />
            <Label text="Re-enter new password:" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <Button fx:id="buttonChange" layoutX="305.0" layoutY="212.0" mnemonicParsing="false" onAction="#handleButtonChange" text="Change" AnchorPane.bottomAnchor="35.0" />
